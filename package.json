{
  "name": "vegian-store",
  "description": "vegian-store food e-commerce website",
  "authors": "Emet Das",
  "version": "1.0.0",
  "main": "index.html",
  "scripts": {
    "start": "node-sass sass/style.scss css/style.css -w",
    "compress:css": "node-sass css/style.css css/compile.css --output-style compressed",
    "dev": "parcel index.html",
    "build": "parcel build index.html --out-dir production",
    "install": "babel js/script.js --watch --out-dir dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.5"
  }
}
